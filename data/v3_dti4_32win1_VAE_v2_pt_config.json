{
  "subjects_filename": "data/subjects_v3.json",
  "surface_names": [
    "CortexLeft",
    "CortexRight"
  ],
  "surface_basename_template": "T1w/fsaverage_LR32k/{subject_id}.{hemi}.white.32k_fs_LR.surf.gii",
  "feature_basenames": "T1w/Diffusion/dti.nii.gz",
  "target_basenames": null,
  "hemispheres": [
    "L",
    "R"
  ],
  "window": [
    32,
    32,
    32
  ],
  "spacing": [1, 1, 1],
  "n_features": 1,
  "optimizer": "Adam",
  "package": "pytorch",
  "loss": "vae_loss",
  "evaluation_metric": "MSELoss",
  "custom_loss": true,
  "regularized": false,
  "vae": true,
  "points_per_subject": 200,
  "reorder": false,
  "validation_points_per_subject": 400,
  "save_best_only": false,
  "n_epochs": 2000,
  "min_learning_rate": 1e-08,
  "decay_patience": 50,
  "decay_factor": 0.5,
  "early_stopping_patience": 100,
  "flip": false,
  "batch_size": 2000,
  "validation_batch_size": 2000,
  "collate_fn": "collate_5d_flatten",
  "initial_learning_rate": 1e-04,
  "model_name": "VariationalAutoEncoder",
  "model_kwargs": {
    "base_width":  32,
    "encoder_blocks": [2, 2, 2, 4],
    "vae_features": 16,
    "decoder_mirrors_encoder": true,
    "input_shape": [32, 32, 32],
    "activation": null
  },
  "skip_validation": false,
  "iterations_per_epoch": 1,
  "n_outputs": 1,
  "sequence": "WindowedAEDataset",
  "n_dense_layers": 1,
  "freeze_bias": false,
  "metric_names": null,
  "sequence_kwargs": {
    "normalization": "zero_one_window",
    "interpolation": "linear"
  },
  "additional_training_args": {}
}